{
  "name": "@pgl-cortex/pgl-search-select",
  "version": "12.2.4",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "start": "ng serve --project=pgl-search-select-tester",
    "docker:start": "ng serve --project=pgl-search-select-tester --host 0.0.0.0 --poll=500",
    "start:watch": "wait-on dist/pgl-search-select/public_api.d.ts && ng serve --project=pgl-search-select-tester --host 0.0.0.0 --poll=500 --watch",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "ng test --project=pgl-search-select --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --project=pgl-search-select --watch=false --code-coverage",
    "lint": "eslint . --ext .ts",
    "e2e": "ng e2e",
    "build:lib": "ng build pgl-search-select --configuration production",
    "build:lib:watch": "ng build pgl-search-select --watch",
    "start:all": "npm run build:lib && npm run docker:start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "npm_pack": "cd dist/pgl-search-select && npm pack",
    "package": "npm run build:lib && npm run npm_pack",
    "watch:all": "npm run clean && run-p build:lib:watch start:watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "^12.2.4",
    "@angular/compiler": "^12.2.4",
    "@angular/core": "^12.2.4",
    "@angular/forms": "^12.2.4",
    "@angular/material": "^12.2.4",
    "@angular/platform-browser": "^12.2.4",
    "@angular/platform-browser-dynamic": "^12.2.4",
    "@angular/router": "^12.2.4",
    "rxjs": "^7.3.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.4",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "^12.2.4",
    "@angular/compiler-cli": "^12.2.4",
    "@angular/language-service": "^12.2.4",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "codelyzer": "^6.0.0",
    "cypress": "^8.3.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^36.0.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "github:prettier/eslint-plugin-prettier",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tsickle": "^0.43.0",
    "typescript": "4.3.5",
    "wait-on": "^6.0.0"
  },
  "author": "Polyglot Labs",
  "license": "MIT",
  "description": "Searchable select",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyglotlabs/pgl-search-select.git"
  },
  "keywords": [
    "Angular",
    "Angular 9",
    "search",
    "select"
  ],
  "bugs": {
    "url": "https://github.com/polyglotlabs/pgl-search-select/issues"
  },
  "homepage": "https://github.com/polyglotlabs/pgl-search-select#readme"
}
