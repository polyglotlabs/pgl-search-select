{
  "name": "@pgl-cortex/pgl-search-select",
  "version": "9.0.1",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "start": "ng serve --project=pgl-search-select-tester",
    "docker:start": "ng serve --project=pgl-search-select-tester --host 0.0.0.0 --poll=500",
    "start:watch": "wait-on dist/pgl-search-select/public_api.d.ts && ng serve --project=pgl-search-select-tester --host 0.0.0.0 --poll=500 --watch",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "ng test --project=pgl-search-select --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --project=pgl-search-select --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build pgl-search-select",
    "build:lib:watch": "ng build pgl-search-select --watch",
    "start:all": "npm run build:lib && npm run docker:start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "npm_pack": "cd dist/pgl-search-select && npm pack",
    "package": "npm run build:lib && npm run npm_pack",
    "watch:all": "npm run clean && run-p build:lib:watch start:watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.0-next.2",
    "@angular-devkit/build-ng-packagr": "^0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.0",
    "codelyzer": "^5.2.2",
    "cypress": "^4.11.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "tsickle": "^0.38.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.2"
  },
  "author": "Polyglot Labs",
  "license": "MIT",
  "description": "Searchable select",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyglotlabs/pgl-search-select.git"
  },
  "keywords": [
    "Angular",
    "Angular 9",
    "search",
    "select"
  ],
  "bugs": {
    "url": "https://github.com/polyglotlabs/pgl-search-select/issues"
  },
  "homepage": "https://github.com/polyglotlabs/pgl-search-select#readme"
}
